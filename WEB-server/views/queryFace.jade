extend default

block content

	p Query a face
	
	form(action='/queryFace', method='post', enctype='multipart/form-data')
		input(type='file', name='displayImage')
		div.actions
			input(type='submit', value='Query Face')
			
	if params.showResponse
		p #{params.response}
		
		if params.match
			p Face ID: #{params.faceID}
			p Database: #{params.databaseID}
		
		
	